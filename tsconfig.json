    "esModuleInterop": true,
    "lib": [
      "es6",
      "dom"
    ],
    "module": "commonjs",
    "outDir": "dist",
    "resolveJsonModule": true,
    "rootDir": "src",
    "skipLibCheck": true,
    "target": "es2016",
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "**/*.test.ts"
  ],
}
